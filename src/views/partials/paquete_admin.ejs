<article>

    <div class="iconos_paquete_admin">
        <img class="img_table" src='/images/<%= paquete.package_image ? paquete.package_image: 'default.png' %>' alt='<%= paquete.package_alt_image%>' >
        <%- include('menu_iconos_admin', {paquete}) %> 
    </div>
    <table class="table">

            <tr>
            <td class="campos_admin"><strong>ID: <%= paquete.package_id%></strong></td>    
            <td class="campos_admin">Nombre: <%= paquete.package_name%></td>
            <td class="campos_admin">Precio: <%= paquete.package_price%></td>
            <td class="campos_admin">Titulo: <%= paquete.package_title%></td>
            <td class="campos_admin">Cantidad de días: <%= paquete.package_q_days%></td>
            <td class="campos_admin">Hotel: <%= paquete.package_hotel%> - <%= paquete.hotel.nombre%></td>
            <td class="campos_admin">Estrellas del Hotel: <%= paquete.hotel.stars%></td>
            <td class="campos_admin">Descripción: <%= paquete.package_description%></td>
            <td class="campos_admin">Fecha de admisión: <%= paquete.package_date_admission%></td>
            <td class="campos_admin">Vidriera del Home: <%= paquete.package_index== "1"? "SI" : "NO"%></td>
            <td class="campos_admin">Categorias: <%= paquete.package_category== "1" ? "Paquete regular" : "Paquete oferta"%></td>
            <td class="campos_admin">Transporte: <%= paquete.package_transportation== "1" ? "Aereo" : paquete.package_transportation== "2" ? "Bus" : "Sin transporte"%></td>
            <td class="campos_admin">Excursiones: <%= paquete.package_excursions_id%> - <%= paquete.tour.name%></td>
            <td class="campos_admin">Descuento: <%= paquete.package_discount%>%</p>
            </div>
        </tr>
    </table>
</article>
<hr>


